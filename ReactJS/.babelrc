{
  "presets": ["es2015", "react"],
  "env": {
    "development": {
      "plugins": [
        "transform-class-properties",
        ["react-transform", {
          "transforms": [{
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            }]
        }]
      ]
    }
  }
}
